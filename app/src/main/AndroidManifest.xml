<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guo.lab">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="androzid.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.CAMERA" />


    <!--define custom permission -->
    <permission
        android:name="Hello"
        android:protectionLevel="dangerous" />
    <permission-group android:name="Hello_Group" />


    <application
        android:name=".LabApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- android:configChanges="orientation|screenSize" -->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HorizontalPagerActivity" />
        <activity android:name=".anim.LayoutAnimationActivity" />
        <activity android:name=".anim.AnimatorActivity" />
        <activity android:name=".recyclerview.RecyclerActivity" />

        <receiver
            android:name=".remoteview.ToastReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".anim.SpringAnimationActivity" />
        <activity android:name=".anim.LayoutTransitionActivity" />
        <activity android:name=".draw.CanvasActivity" />
        <activity android:name=".recyclerview.ExpanableRecyclerActivity" />
        <activity android:name=".battery.TextActivity" />
        <activity android:name=".draw.ClipDrawableActivity" />
        <activity android:name=".anim.RingActivity" />
        <activity android:name=".constraint.ConstraintActivity" />
        <activity
            android:name=".material.DrawerActivity"
            android:label="@string/title_activity_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".widget.DragScrollActivity" />
        <activity android:name=".widget.SlidingPanelActivity" />
        <activity android:name=".widget.ComposedViewActivity" />
        <activity android:name=".databinding.DataBindingActivity" />
        <activity android:name=".lottie.LottieActivity" />
        <activity android:name=".dimention.DimensionActivity" />
        <activity android:name=".scroll.ScrollActivity" />
        <activity android:name=".permission.PermissionActivity"></activity>
        <activity android:name=".databinding.BindingActivity" />
        <activity android:name=".databinding.ListBindingActivity" />
    </application>

</manifest>