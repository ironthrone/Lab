<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guo.lab">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="androzid.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- <uses-permission android:name="android.permission.DELETE_CACHE_FILES" /> -->
    <!-- <uses-permission android:name="android.permission.WRITE_SECURE_SETTINGS" /> -->
    <!-- <uses-permission android:name="android.permission.MODIFY_PHONE_STATE" /> -->
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <!-- <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" /> -->
    <!-- define custom permission -->
    <permission
        android:name="Hello"
        android:protectionLevel="dangerous" />

    <permission-group android:name="Hello_Group" />

    <application
        android:name=".LabApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- android:configChanges="orientation|screenSize" -->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HorizontalPagerActivity" />
        <activity android:name=".anim.LayoutAnimationActivity" />
        <activity android:name=".anim.AnimatorActivity" />
        <activity android:name=".recyclerview.RecyclerActivity" />

        <receiver
            android:name=".remoteview.ToastReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".anim.SpringAnimationActivity" />
        <activity android:name=".anim.LayoutTransitionActivity" />
        <activity android:name=".draw.CanvasActivity" />
        <activity android:name=".recyclerview.ExpanableRecyclerActivity" />
        <activity android:name=".battery.TextActivity" />
        <activity android:name=".draw.ClipDrawableActivity" />
        <activity android:name=".anim.AnimatorActivity2" />
        <activity android:name=".constraint.ConstraintActivity" />
        <activity
            android:name=".material.DrawerActivity"
            android:label="@string/title_activity_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".widget.DragScrollActivity" />
        <activity android:name=".widget.SlidingPanelActivity" />
        <activity android:name=".widget.ComposedViewActivity" />
        <activity android:name=".databinding.DataBindingActivity" />
        <activity android:name=".lottie.LottieActivity" />
        <activity android:name=".dimention.DimensionActivity" />
        <activity android:name=".scroll.ScrollActivity" />
        <activity android:name=".permission.PermissionActivity" />
        <activity android:name=".databinding.BindingActivity" />
        <activity android:name=".databinding.ListBindingActivity" />

        <service
            android:name=".accessibility.TheAccessibilityService"
            android:label="@string/app_name"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/accessibility_service_info" />
        </service>

        <activity android:name=".accessibility.WindowActivity" />

        <service
            android:name=".accessibility.WindowService"
            android:enabled="true"
            android:exported="true" />

        <!-- android:theme="@style/AppTheme.NoActionBar.FullScreen" -->
        <activity android:name=".view.SomeViewAttrsActivity" />
        <activity android:name=".media.MediaActivity" />
        <activity android:name=".system.ProcessActivity" />
        <activity android:name=".persist.SqliteDatabaseActivity" />
        <activity android:name=".system.SystemSettingActivity" />
        <activity android:name=".navigation.AlphaActivity" />
        <activity
            android:name=".navigation.BetaActivity"
            android:parentActivityName=".navigation.AlphaActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".navigation.AlphaActivity" />
        </activity>
        <activity
            android:name=".navigation.GamaActivity"
            android:parentActivityName=".navigation.BetaActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".navigation.BetaActivity" />
        </activity>
        <activity android:name=".storage.StorageActivity" />
        <activity android:name=".notification.NotificationActivity" />

        <service
            android:name=".storage.CleanCacheAccessibilityService"
            android:label="CleanCache"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/clean_cache_accessi_info" />
        </service>

        <activity
            android:name=".anim.AActivity"
            android:hardwareAccelerated="true" />
        <activity android:name=".dialog.DialogActivity" />

        <receiver android:name=".dialog.OpenDialogReceiver">
            <intent-filter>
                <action android:name="show_dialog" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".accessibility.CleanActivity"
            android:theme="@style/AppTheme.NoActionBar.FullScreen" />
        <activity android:name=".anim.InvalidateTestActivity" />
        <activity android:name=".transition.TransitionFromActivity" />
        <activity android:name=".transition.TransitionToActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>